{
  "name": "reviz",
  "version": "0.1.0",
  "description": "An engine for reverse engineering data visualizations from the DOM.",
  "main": "index.js",
  "contributors": [
    {
      "name": "Parker Ziegler",
      "email": "peziegler@berkeley.edu",
      "url": "https://parkie-doo.sh/"
    },
    {
      "name": "Logan Caraco",
      "email": "logan_caraco@berkeley.edu"
    }
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn check:ts && esbuild src/index.ts --bundle --outdir=dist",
    "check:ts": "tsc --noEmit",
    "format": "prettier --write **/*.{ts,tsx} **/*.json",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "d3-array": "^3.1.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.groupby": "^4.6.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.overevery": "^4.7.0"
  },
  "devDependencies": {
    "@types/d3-array": "^3.0.2",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.groupby": "^4.6.6",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/lodash.overevery": "^4.7.6",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "esbuild": "^0.13.8",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "overrides": [
      {
        "files": [
          "**/*.css"
        ],
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
